{
    "name": "k8s",
    "displayName": "k8s",
    "description": "Kubernetes Developer Tools",
    "version": "0.0.1",
    "publisher": "r2d4",
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:k8s.skaffoldRun"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "k8s.skaffoldRun",
                "title": "Skaffold Run"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
